<view class="content"><view class="header-wrapper"><text class="shop">{{"由"+shopName+"送出"}}</text><view class="list"><block a:for="{{bannerMenu}}" a:for-item="item" a:for-index="idx" a:key="idx"><view class="list-item"><image class="icon" src="{{item.imgUrl}}" mode="aspectFit"></image><view class="text">{{item.text||'aaaaa'}}</view><view class="append">{{item.append||'ds'}}</view></view></block></view></view><swiper class="swiper-wrapper" autoplay="true"><block a:for="{{swiperImgs}}" a:for-item="item" a:for-index="idx" a:key="idx"><swiper-item><image class="swiper-item" src="{{item}}" mode="aspectFit"></image></swiper-item></block></swiper><view class="recommand-wrapper"><text class="section-title">好吃推荐</text><scroll-view class="scroll-wrapper" scroll-x="true"><block a:for="{{recommendList}}" a:for-item="item" a:for-index="idx" a:key="idx"><view class="recommend-item"><image class="icon" src="{{item.imgUrl}}" mode="aspectFit"></image><view class="text">{{item.text}}</view><view class="append">{{item.append}}</view><view class="discount-price">{{"¥"+item.discountPrice}}</view><view class="origin-price">{{"¥"+item.originalPrice}}</view><view data-event-opts="{{[['tap',[['shoppingHandler',['$0'],[[['recommendList','',idx]]]]]]]}}" class="item-cart-wrapper" onTap="__e"><image class="item-cart" src="../../static/icon/shopping.png" mode="contain"></image></view></view></block></scroll-view></view><view class="select-wrapper"><text class="section-title">为你优选</text><view><block a:for="{{recommendList}}" a:for-item="item" a:for-index="idx" a:key="idx"><view class="recommend-item select-item"><image class="icon" src="{{item.imgUrl}}" mode="aspectFit"></image><view class="text">{{item.text}}</view><view class="append">{{item.append}}</view><view class="discount-price">{{"¥"+item.discountPrice}}</view><view class="origin-price">{{"¥"+item.originalPrice}}</view><view data-event-opts="{{[['tap',[['shoppingHandler',['$0'],[[['recommendList','',idx]]]]]]]}}" class="item-cart-wrapper" onTap="__e"><image class="item-cart" src="../../static/icon/shopping.png" mode="contain"></image></view></view></block></view></view><view data-event-opts="{{[['tap',[['viewCart',['$event']]]]]}}" class="cart-icon-wrapper" onTap="__e"><view class="cart-quantity">{{quantityCount}}</view><image class="cart-icon" src="../../static/icon/shopping.png" mode="contain"></image></view></view>